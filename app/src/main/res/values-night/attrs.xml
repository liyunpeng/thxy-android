<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable  name="inputview">
        <!--图片-->
        <attr name="input_icon" format="reference"></attr>
        <!--字体-->
        <attr name="input_hint" format="string"></attr>
        <!--是否密文-->
        <attr name="is_pass" format="boolean"></attr>
    </declare-styleable>


    <declare-styleable  name="nav">
        <!--图片-->
        <attr name="icon" format="reference"></attr>
        <!--字体-->
        <attr name="hint" format="string"></attr>
        <!--是否密文-->
        <attr name="pass" format="boolean"></attr>
        <attr name="right_icon" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="UniversalMediaController">
        <attr name="uvv_scalable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="UniversalVideoView">
        <attr name="uvv_fitXY" format="boolean" />
        <attr name="uvv_autoRotation" format="boolean" />
    </declare-styleable>
</resources>